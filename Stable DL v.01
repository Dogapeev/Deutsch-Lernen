                    { german: "die Zerrissenheit", russian: "раздвоенность", pronunciation: "[ди церрі́сенхайт]", level: "B2", theme: "emotions", morphemes: [{m:'Zer', t:'раз-'}, {m:'riss', t:'рвать'}, {m:'en', t:'-'}, {m:'heit', t:'-ость'}], sentence: "Ihre innere Zerrissenheit war offensichtlich.", sentence_ru: "Ее внутренняя раздвоенность была очевидна." },
                    { german: "die Ausgeglichenheit", russian: "уравновешенность", pronunciation: "[ди а́усгегліхенхайт]", level: "B2", theme: "emotions", morphemes: [{m:'Aus', t:'из/окончание'}, {m:'ge', t:'-'}, {m:'glich', t:'равный'}, {m:'en', t:'-'}, {m:'heit', t:'-ость'}], sentence: "Yoga hilft bei der Ausgeglichenheit.", sentence_ru: "Йога помогает достичь уравновешенности." },
                    { german: "die Empathiefähigkeit", russian: "способность к эмпатии", pronunciation: "[ди эмпа́тифехигкайт]", level: "B2", theme: "emotions", morphemes: [{m:'Empathie',t:'эмпатия'}, {m:'fähig',t:'способный'}, {m:'keit',t:'-ость'}], sentence: "Empathiefähigkeit ist eine wichtige soziale Kompetenz.", sentence_ru: "Способность к эмпатии - важная социальная компетенция." },
                    { german: "die Selbstbeherrschung", russian: "самообладание", pronunciation: "[ди зе́льпстбехершунг]", level: "B2", theme: "emotions", morphemes: [{m:'Selbst', t:'само-'}, {m:'be', t:'-'}, {m:'herrsch', t:'властвовать'}, {m:'ung', t:'суффикс'}], sentence: "In dieser Situation ist Selbstbeherrschung gefragt.", sentence_ru: "В этой ситуации требуется самообладание." },
                    { german: "die Ungewissheit", russian: "неопределенность", pronunciation: "[ди у́нгевисхайт]", level: "B2", theme: "emotions", morphemes: [{m:'Un', t:'не-'}, {m:'ge', t:'-'}, {m:'wiss', t:'знать'}, {m:'heit', t:'-ость'}], sentence: "Die Ungewissheit über die Zukunft macht Angst.", sentence_ru: "Неопределенность будущего пугает." }
                ];
                
                console.log(`Загружен стандартный словарь: ${this.allWords.length} слов`);
                this.updateStats();
                this.saveToLocalStorage();
                this.showMessage('Загружен стандартный словарь: ' + this.allWords.length + ' слов');
            }

            bindEvents() {
                document.addEventListener('click', () => this.initializeSpeech(), { once: true });
                document.getElementById('soundToggle').addEventListener('click', () => this.toggleSound());
                document.getElementById('translationSoundToggle').addEventListener('click', () => this.toggleTranslationSound());
                document.getElementById('sentenceSoundToggle').addEventListener('click', () => this.toggleSentenceSound());
                document.getElementById('toggleArticles').addEventListener('click', () => this.toggleArticles());
                document.getElementById('toggleMorphemes').addEventListener('click', () => this.toggleMorphemes());
                document.getElementById('toggleMorphemeTranslations').addEventListener('click', () => this.toggleMorphemeTranslations());
                document.getElementById('toggleSentences').addEventListener('click', () => this.toggleSentences());
                document.getElementById('reloadDefaultWords').addEventListener('click', () => this.reloadDefaultWords());
                document.querySelectorAll('.repeat-selector').forEach(btn => !btn.id.startsWith('toggle') && btn.addEventListener('click', (e) => this.setRepeatMode(e.target.dataset.mode)));
                document.querySelectorAll('.level-btn').forEach(btn => btn.addEventListener('click', (e) => this.toggleLevel(e.target.dataset.level)));
                document.querySelectorAll('.block-btn').forEach(btn => btn.addEventListener('click', (e) => this.setTheme(e.target.dataset.theme)));
                document.getElementById('toggleButton').addEventListener('click', () => this.toggleAutoPlay());
                document.getElementById('prevButton').addEventListener('click', () => this.showPreviousWord());
                document.getElementById('nextButton').addEventListener('click', () => this.showNextWordManually());
                document.getElementById('exportWords').addEventListener('click', () => this.exportWords());
                document.getElementById('importWords').addEventListener('click', () => document.getElementById('fileInput').click());
                document.getElementById('fileInput').addEventListener('change', (e) => this.importWords(e));
                document.getElementById('loadFromGithub').addEventListener('click', () => this.loadFromGithub());
            }

            getNextWord() {
                const activeWords = this.getActiveWords();
                if (activeWords.length === 0) return null;
                if (this.repeatMode === 'random') {
                    return activeWords[Math.floor(Math.random() * activeWords.length)];
                }
                const currentIndex = activeWords.findIndex(w => this.currentWord && w.german === this.currentWord.german);
                const nextIndex = (currentIndex + 1) % activeWords.length;
                return activeWords[nextIndex];
            }

            getActiveWords() {
                return this.allWords.filter(w => this.selectedLevels.includes(w.level) && (this.selectedTheme === 'all' || w.theme === this.selectedTheme));
            }

            parseGermanWord(word) {
                const german = word.german;
                if (german.startsWith('der ')) return { article: 'der', mainWord: german.substring(4), genderClass: 'der' };
                if (german.startsWith('die ')) return { article: 'die', mainWord: german.substring(4), genderClass: 'die' };
                if (german.startsWith('das ')) return { article: 'das', mainWord: german.substring(4), genderClass: 'das' };
                return { article: null, mainWord: german, genderClass: 'das' };
            }
            
            formatGermanWord(word) {
                const parsed = this.parseGermanWord(word);
                const articleClass = this.showArticles ? '' : 'hide-articles';
                const mainWordHtml = `<span class="morpheme">${parsed.mainWord}</span>`;
                const articleHtml = parsed.article ? `<span class="article ${parsed.genderClass}">${parsed.article}</span>` : '';
                return `<div class="word ${parsed.genderClass} ${articleClass}">${articleHtml}<span class="main-word">${mainWordHtml}</span></div>`;
            }

            startAutoPlay() {
                this.isAutoPlaying = true;
                this.showNextWordInSequence();
                this.updateToggleButton();
            }

            stopAutoPlay() {
                this.isAutoPlaying = false;
                clearTimeout(this.autoPlayTimeout);
                this.autoPlayTimeout = null;
                this.updateToggleButton();
            }

            showNextWordInSequence() {
                if (!this.isAutoPlaying) return;
                const activeWords = this.getActiveWords();
                if (activeWords.length === 0) {
                    document.getElementById('studyArea').innerHTML = `<div class="no-words"><p>Нет слов для выбранных уровней и темы</p></div>`;
                    return;
                }
                this.currentWord = this.getNextWord();
                if (this.currentWord) this.renderWord(this.currentWord, true);
            }

            renderWord(word, withAnimation = false) {
                if (this.isAutoPlaying) this.addToHistory(word);
                
                const studyArea = document.getElementById('studyArea');
                const animationClass = withAnimation ? 'card-appear' : '';

                studyArea.innerHTML = `
                    <div class="card ${animationClass}" id="wordCard">
                        <div class="level-indicator ${word.level.toLowerCase()}">${word.level}</div>
                        <div class="word-container">
                            ${this.formatGermanWord(word)}
                            <div class="pronunciation" style="opacity: 0.7;">${word.pronunciation || ''}</div>
                            <div id="translationContainer"></div>
                            <div id="morphemeTranslations" class="morpheme-translations"></div>
                            <div id="sentenceContainer"></div>
                        </div>
                    </div>`;

                const cardElement = document.getElementById('wordCard');
                if (cardElement) {
                    cardElement.addEventListener('click', () => this.toggleAutoPlay());
                    if (this.isAutoPlaying) cardElement.classList.add('is-clickable');
                }
                speechSynthesis.cancel();
                this.repeatGermanPart(word, 1);
                this.updateNavigationButtons();
            }
            
            repeatGermanPart(word, count) {
                if (!this.isAutoPlaying || !document.getElementById('wordCard')) return;
                this.speakGerman(word.german);
                const totalRepeats = this.repeatMode === 'random' ? 1 : parseInt(this.repeatMode, 10);
                if (count < totalRepeats) {
                    this.autoPlayTimeout = setTimeout(() => this.repeatGermanPart(word, count + 1), 3000);
                } else {
                    this.autoPlayTimeout = setTimeout(() => this.startTransformation(word), 2000);
                }
            }

            startTransformation(word) {
                const card = document.getElementById('wordCard');
                if (!card) return;
                card.classList.add('transform-phase');
                
                this.autoPlayTimeout = setTimeout(() => {
                    if (!document.body.contains(card)) return;
                    if (word.morphemes && this.showMorphemes) this.displayMorphemesAndTranslations(word);
                    card.classList.add('morpheme-phase');

                    this.autoPlayTimeout = setTimeout(() => {
                        if (!document.body.contains(card)) return;
                        if (word.sentence && this.showSentences) this.displaySentence(word);
                        card.classList.add('sentence-phase');

                        this.autoPlayTimeout = setTimeout(() => {
                            if (!document.body.contains(card)) return;
                            this.showTranslation(word);
                        }, 4000);
                    }, 2500);
                }, 2000);
            }

            displaySentence(word) {
                const container = document.getElementById('sentenceContainer');
                if (!container) return;
                container.innerHTML = `
                    <div class="sentence sentence-appear">
                        ${word.sentence}
                        <div class="sentence-translation">${word.sentence_ru}</div>
                    </div>
                `;
                if(this.sentenceSoundEnabled) {
                    setTimeout(() => this.speakGerman(word.sentence), 500);
                }
            }
            
            showTranslation(word) {
                const card = document.getElementById('wordCard');
                if (!card || !document.body.contains(card)) return;
                card.classList.remove('transform-phase', 'morpheme-phase', 'sentence-phase', 'zoom-phase');
                card.classList.add('final-phase');
                this.studiedToday++;
                this.updateStats();
                this.saveToLocalStorage();

                const translationContainer = document.getElementById('translationContainer');
                if (translationContainer) {
                    const translationElement = document.createElement('div');
                    translationElement.className = 'translation translation-appear';
                    translationElement.textContent = word.russian;
                    translationContainer.innerHTML = '';
                    translationContainer.appendChild(translationElement);
                    if(this.translationSoundEnabled) {
                        setTimeout(() => this.speakRussian(word.russian), 500);
                    }
                }
                
                this.autoPlayTimeout = setTimeout(() => this.isAutoPlaying && this.transitionToNextWord(), 5000);
            }

            transitionToNextWord() {
                const mainContent = document.getElementById('mainContent');
                const card = document.getElementById('wordCard');
                if (!card) { this.showNextWordInSequence(); return; }
                mainContent.style.minHeight = `${card.offsetHeight}px`;
                card.classList.add('word-crossfade', 'word-fade-out');
                this.autoPlayTimeout = setTimeout(() => {
                    this.showNextWordInSequence();
                    setTimeout(() => { mainContent.style.minHeight = ''; }, 100);
                }, 750);
            }

            displayMorphemesAndTranslations(word) {
                const mainWordElement = document.querySelector('.word .main-word');
                const translationsContainer = document.getElementById('morphemeTranslations');
                const wordElement = document.querySelector('.word');
                if (!mainWordElement || !translationsContainer || !wordElement || !word.morphemes || !Array.isArray(word.morphemes)) return;
                mainWordElement.innerHTML = word.morphemes.map(item => `<span class="morpheme">${item.m}</span>`).join('<span class="morpheme-separator">-</span>');
                setTimeout(() => wordElement.classList.add('show-morphemes'), 10);
                if (this.showMorphemeTranslations) {
                    translationsContainer.innerHTML = word.morphemes.map(item => `<div class="morpheme-translation-item"><span class="morpheme-part">${item.m}</span><span class="translation-part">${item.t || '?'}</span></div>`).join('');
                    setTimeout(() => translationsContainer.classList.add('visible'), 10);
                }
            }
            
            reloadDefaultWords() { localStorage.clear(); window.location.reload(); }

            initializeSpeech() {
                if ('speechSynthesis' in window && !this.speechInitialized) {
                    speechSynthesis.speak(new SpeechSynthesisUtterance('')); this.speechInitialized = true;
                }
            }

            speak(text, lang, rate) {
                if (!('speechSynthesis' in window)) return;
                const utterance = new SpeechSynthesisUtterance(text);
                utterance.lang = lang; utterance.rate = rate;
                speechSynthesis.speak(utterance);
            }

            speakGerman(text) { if (this.soundEnabled) this.speak(text, 'de-DE', 0.8); }
            speakRussian(text) { if (this.translationSoundEnabled) this.speak(text, 'ru-RU', 0.9); }
            
            updateStats() {
                document.getElementById('totalWords').textContent = this.getActiveWords().length;
                document.getElementById('studiedToday').textContent = this.studiedToday;
                const acc = this.accuracy.total > 0 ? Math.round((this.accuracy.correct / this.accuracy.total) * 100) : 0;
                document.getElementById('accuracy').textContent = acc + '%';
            }

            addToHistory(word) {
                if (this.currentHistoryIndex < this.wordHistory.length - 1) this.wordHistory.splice(this.currentHistoryIndex + 1);
                this.wordHistory.push(word);
                if (this.wordHistory.length > 50) this.wordHistory.shift();
                this.currentHistoryIndex = this.wordHistory.length - 1;
            }

            showPreviousWord() {
                if (this.isAutoPlaying) this.stopAutoPlay();
                if (this.currentHistoryIndex > 0) {
                    this.currentHistoryIndex--;
                    this.renderWordDirectly(this.wordHistory[this.currentHistoryIndex]);
                }
            }

            showNextWordManually() {
                if (this.isAutoPlaying) this.stopAutoPlay();
                if (this.currentHistoryIndex < this.wordHistory.length - 1) {
                    this.currentHistoryIndex++;
                    this.renderWordDirectly(this.wordHistory[this.currentHistoryIndex]);
                } else {
                    const nextWord = this.getNextWord();
                    if (nextWord) { this.addToHistory(nextWord); this.renderWordDirectly(nextWord); }
                }
            }
            
            renderWordDirectly(word) {
                this.currentWord = word;
                const studyArea = document.getElementById('studyArea');
                const oldCard = document.getElementById('wordCard');
                
                const renderNew = () => {
                    studyArea.innerHTML = `
                        <div class="card card-appear" id="wordCard">
                           <div class="level-indicator ${word.level.toLowerCase()}">${word.level}</div>
                            <div class="word-container">
                                ${this.formatGermanWord(word)}
                                <div class="pronunciation" style="opacity: 0.7;">${word.pronunciation || ''}</div>
                                <div id="translationContainer"><div class="translation">${word.russian}</div></div>
                                <div id="morphemeTranslations" class="morpheme-translations"></div>
                                <div id="sentenceContainer"></div>
                            </div>
                        </div>`;
                    
                    document.getElementById('wordCard').addEventListener('click', () => this.toggleAutoPlay());
                    this.updateMorphemeDisplayOnCard(true);
                    this.updateSentenceDisplayOnCard();

                    speechSynthesis.cancel();
                    setTimeout(() => {
                        this.speakGerman(word.german);
                        if(this.showSentences && word.sentence && this.sentenceSoundEnabled) {
                           setTimeout(() => this.speakGerman(word.sentence), 1500);
                           setTimeout(() => this.speakRussian(word.russian), 3500);
                        } else {
                           setTimeout(() => this.speakRussian(word.russian), 1500);
                        }
                    }, 300);
                };

                if (oldCard) {
                    oldCard.classList.add('word-crossfade', 'word-fade-out');
                    setTimeout(renderNew, 300); 
                } else {
                    renderNew();
                }
                this.updateNavigationButtons();
            }
            
            // --- UI Update Methods ---
            updateNavigationButtons() {
                document.getElementById('prevButton').disabled = this.wordHistory.length === 0 || this.currentHistoryIndex <= 0;
                document.getElementById('nextButton').disabled = this.getActiveWords().length === 0;
            }
            toggleAutoPlay() { this.isAutoPlaying ? this.stopAutoPlay() : this.startAutoPlay(); }
            updateToggleButton() {
                const btn = document.getElementById('toggleButton');
                if (btn) {
                    const icon = this.isAutoPlaying ? '#icon-pause' : '#icon-play';
                    btn.innerHTML = `<svg class="icon" width="22px" height="22px"><use xlink:href="${icon}"></use></svg>`;
                    btn.classList.toggle('playing', this.isAutoPlaying);
                }
            }
            updateSoundButton() {
                const btn = document.getElementById('soundToggle');
                const icon = this.soundEnabled ? '#icon-sound-on' : '#icon-sound-off';
                btn.innerHTML = `<svg class="icon" width="20px" height="20px"><use xlink:href="${icon}"></use></svg>`;
                btn.classList.toggle('enabled', this.soundEnabled);
            }
            updateTranslationSoundButton() {
                const btn = document.getElementById('translationSoundToggle');
                const icon = this.translationSoundEnabled ? '#icon-chat-on' : '#icon-chat-off';
                btn.innerHTML = `<svg class="icon" width="20px" height="20px"><use xlink:href="${icon}"></use></svg>`;
                btn.classList.toggle('enabled', this.translationSoundEnabled);
            }
            updateSentenceSoundButton() {
                const btn = document.getElementById('sentenceSoundToggle');
                const icon = this.sentenceSoundEnabled ? '#icon-sentence-on' : '#icon-sentence-off';
                btn.innerHTML = `<svg class="icon" width="20px" height="20px"><use xlink:href="${icon}"></use></svg>`;
                btn.classList.toggle('enabled', this.sentenceSoundEnabled);
            }

            updateRepeatButtons() { document.querySelectorAll('.repeat-selector[data-mode]').forEach(btn => btn.classList.toggle('active', btn.dataset.mode === this.repeatMode)); }
            updateLevelButtons() { document.querySelectorAll('.level-btn').forEach(btn => btn.classList.toggle('active', this.selectedLevels.includes(btn.dataset.level))); }
            updateThemeButtons() { document.querySelectorAll('.block-btn').forEach(btn => btn.classList.toggle('active', btn.dataset.theme === this.selectedTheme)); }
            updateArticleButton() { document.getElementById('toggleArticles')?.classList.toggle('active', this.showArticles); }
            updateMorphemeButton() { document.getElementById('toggleMorphemes')?.classList.toggle('active', this.showMorphemes); }
            updateMorphemeTranslationsButton() {
                const btn = document.getElementById('toggleMorphemeTranslations');
                if (!btn) return;

                if (!this.showMorphemes) {
                    btn.classList.remove('active');
                    btn.disabled = true;
                } else {
                    btn.disabled = false;
                    btn.classList.toggle('active', this.showMorphemeTranslations);
                }
            }
            updateSentenceButton() { document.getElementById('toggleSentences')?.classList.toggle('active', this.showSentences); }

            // --- Toggle Methods ---
            toggleSound() { this.soundEnabled = !this.soundEnabled; this.saveToLocalStorage(); this.updateSoundButton(); }
            toggleTranslationSound() { this.translationSoundEnabled = !this.translationSoundEnabled; this.saveToLocalStorage(); this.updateTranslationSoundButton(); }
            toggleSentenceSound() { this.sentenceSoundEnabled = !this.sentenceSoundEnabled; this.saveToLocalStorage(); this.updateSentenceSoundButton(); }
            toggleArticles() {
                this.showArticles = !this.showArticles; this.saveToLocalStorage(); this.updateArticleButton();
                document.querySelector('#wordCard .word')?.classList.toggle('hide-articles', !this.showArticles);
            }
            toggleMorphemes() {
                this.showMorphemes = !this.showMorphemes;
                if (!this.showMorphemes) {
                    this.showMorphemeTranslations = false;
                }
                this.saveToLocalStorage();
                this.updateMorphemeButton();
                this.updateMorphemeTranslationsButton();
                this.updateMorphemeDisplayOnCard();
            }
            toggleMorphemeTranslations() {
                if (!this.showMorphemes) return; // Нельзя включить, если морфемы выключены
                this.showMorphemeTranslations = !this.showMorphemeTranslations;
                this.saveToLocalStorage();
                this.updateMorphemeTranslationsButton();
                this.updateMorphemeDisplayOnCard();
            }
            toggleSentences() {
                this.showSentences = !this.showSentences; this.saveToLocalStorage(); this.updateSentenceButton();
                this.updateSentenceDisplayOnCard();
            }

            updateMorphemeDisplayOnCard(isDirect = false) {
                const word = this.currentWord; if (!word || !document.getElementById('wordCard')) return;
                const wordElement = document.querySelector('.word'); const mainWordElement = document.querySelector('.word .main-word'); const translationsContainer = document.getElementById('morphemeTranslations');
                if (!wordElement || !mainWordElement || !translationsContainer) return;
                const parsed = this.parseGermanWord(word); mainWordElement.innerHTML = `<span class="morpheme">${parsed.mainWord}</span>`;
                wordElement.classList.remove('show-morphemes'); translationsContainer.classList.remove('visible'); translationsContainer.innerHTML = '';
                if (this.showMorphemes && word.morphemes && Array.isArray(word.morphemes)) {
                    mainWordElement.innerHTML = word.morphemes.map(item => `<span class="morpheme">${item.m}</span>`).join('<span class="morpheme-separator">-</span>');
                    if (isDirect) {
                        wordElement.classList.add('show-morphemes');
                        if (this.showMorphemeTranslations) {
                            translationsContainer.innerHTML = word.morphemes.map(item => `<div class="morpheme-translation-item"><span class="morpheme-part">${item.m}</span><span class="translation-part">${item.t || '?'}</span></div>`).join('');
                            translationsContainer.classList.add('visible');
                        }
                    }
                }
            }
            
            updateSentenceDisplayOnCard() {
                const word = this.currentWord; const container = document.getElementById('sentenceContainer');
                if (!word || !container) return;
                if (this.showSentences && word.sentence) {
                    container.innerHTML = `<div class="sentence">${word.sentence}<div class="sentence-translation">${word.sentence_ru}</div></div>`;
                } else {
                    container.innerHTML = '';
                }
            }

            setRepeatMode(mode) { this.repeatMode = mode; this.saveToLocalStorage(); this.updateRepeatButtons(); this.restartAutoPlay(); }
            toggleLevel(level) {
                const index = this.selectedLevels.indexOf(level);
                if (index > -1) this.selectedLevels.splice(index, 1); else this.selectedLevels.push(level);
                if (this.selectedLevels.length === 0) this.selectedLevels.push(level);
                this.saveToLocalStorage(); this.updateLevelButtons(); this.updateStats(); this.restartAutoPlay();
            }
            setTheme(theme) { this.selectedTheme = theme; this.saveToLocalStorage(); this.updateThemeButtons(); this.updateStats(); this.restartAutoPlay(); }
            restartAutoPlay() { if (this.isAutoPlaying) { this.stopAutoPlay(); this.autoPlayTimeout = setTimeout(() => this.startAutoPlay(), 100); } else { this.currentWord = null; this.showNextWordManually(); } }

            saveToLocalStorage() {
                const state = {
                    selectedLevels: this.selectedLevels, selectedTheme: this.selectedTheme, studiedToday: this.studiedToday, 
                    soundEnabled: this.soundEnabled, translationSoundEnabled: this.translationSoundEnabled, sentenceSoundEnabled: this.sentenceSoundEnabled,
                    repeatMode: this.repeatMode, showArticles: this.showArticles, showMorphemes: this.showMorphemes, 
                    showMorphemeTranslations: this.showMorphemeTranslations, showSentences: this.showSentences, 
                    accuracy: this.accuracy
                };
                for (const key in state) { localStorage.setItem(key, JSON.stringify(state[key])); }
                localStorage.setItem('germanWords', JSON.stringify(this.allWords));
            }

            showMessage(text) {
                const statusDiv = document.getElementById('levelStatus');
                if (statusDiv) {
                    statusDiv.textContent = text;
                    statusDiv.style.display = 'block';
                    setTimeout(() => { statusDiv.style.display = 'none'; }, 3000);
                }
            }
            
            async loadFromGithub() { this.showMessage('GitHub словари пока недоступны.'); }
            exportWords() {
                const blob = new Blob([JSON.stringify(this.allWords, null, 2)], {type: 'application/json'});
                const a = document.createElement('a');
                a.href = URL.createObjectURL(blob); a.download = `german-vocabulary-${new Date().toISOString().split('T')[0]}.json`;
                a.click(); URL.revokeObjectURL(a.href); this.showMessage('Словарь экспортирован!');
            }
            importWords(event) {
                const file = event.target.files[0]; if (!file) return;
                const reader = new FileReader();
                reader.onload = (e) => {
                    try {
                        const imported = JSON.parse(e.target.result);
                        if (Array.isArray(imported) && imported.length > 0 && imported[0].german && imported[0].russian) {
                            this.allWords = [...this.allWords, ...imported];
                            this.updateStats(); this.saveToLocalStorage(); this.showMessage(`Импортировано ${imported.length} слов!`);
                        } else { this.showMessage('Неверный формат файла.'); }
                    } catch (error) { this.showMessage('Ошибка чтения файла: ' + error.message); }
                };
                reader.readAsText(file); event.target.value = '';
            }
        }

        document.addEventListener('DOMContentLoaded', () => {
            try { 
                window.app = new VocabularyApp(); 
                console.log('✅ Приложение (Морфемы + Предложения) успешно инициализировано.'); 
            } 
