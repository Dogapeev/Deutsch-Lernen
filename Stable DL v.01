<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Изучение немецких слов - Морфемы и Предложения</title>
    <style>
        * { margin: 0; padding: 0; box-sizing: border-box; }
        body { font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif; background: #fafafa; min-height: 100vh; padding: 20px; color: #333; line-height: 1.6; }
        .container { max-width: 900px; margin: 0 auto; background: white; border-radius: 8px; box-shadow: 0 2px 8px rgba(0,0,0,0.05); overflow: hidden; }
        .header { background: #fff; border-bottom: 1px solid #e5e5e5; padding: 40px 30px; text-align: center; }
        .header h1 { font-size: 2.5rem; margin-bottom: 8px; font-weight: 300; color: #333; }
        .header p { color: #666; font-size: 1rem; font-weight: 400; }
        .stats { display: grid; grid-template-columns: repeat(auto-fit, minmax(120px, 1fr)); gap: 1px; background: #e5e5e5; padding: 0; }
        .stat { background: white; padding: 30px 20px; text-align: center; }
        .stat-number { font-size: 2rem; font-weight: 600; color: #333; margin-bottom: 4px; }
        .stat-label { color: #666; font-size: 0.85rem; text-transform: uppercase; letter-spacing: 0.5px; font-weight: 500; }
        .level-selector { background: #fff; padding: 25px; border-bottom: 1px solid #e5e5e5; display: flex; justify-content: space-between; align-items: center; flex-wrap: wrap; gap: 20px; }
        .level-buttons { display: flex; gap: 12px; align-items: center; flex-wrap: wrap; }
        .level-btn { padding: 8px 16px; border: 1px solid #ddd; border-radius: 4px; background: white; font-weight: 500; cursor: pointer; transition: all 0.2s ease; font-size: 0.9rem; color: #666; }
        .level-btn:hover { border-color: #999; }
        .level-btn.active { background: #333; color: white; border-color: #333; }
        .block-selector { background: white; padding: 25px; border-bottom: 1px solid #e5e5e5; }
        .block-buttons { display: flex; gap: 8px; align-items: center; flex-wrap: wrap; justify-content: center; }
        .block-btn { padding: 6px 12px; border: 1px solid #ddd; border-radius: 4px; background: white; font-weight: 400; cursor: pointer; transition: all 0.2s ease; font-size: 0.85rem; color: #666; }
        .block-btn:hover { border-color: #999; }
        .block-btn.active { background: #333; color: white; border-color: #333; }
        .block-label { font-weight: 500; color: #333; margin-right: 15px; font-size: 0.9rem; display: flex; align-items: center; }
        .block-label .icon { margin-right: 8px; }
        .main-content { padding: 60px 40px; min-height: 450px; background: white; }
        
        .card {
            background: white;
            border: 1px solid #e5e5e5;
            border-radius: 4px;
            padding: 50px 40px;
            text-align: center;
            margin-bottom: 30px;
            position: relative;
            min-height: 350px; 
            display: flex;
            flex-direction: column;
            justify-content: center;
            transition: all 1s cubic-bezier(0.25, 0.46, 0.45, 0.94), min-height 0.6s ease-out;
        }

        .card.is-clickable { cursor: pointer; }
        .word-container { position: relative; transition: all 3s cubic-bezier(0.23, 1, 0.32, 1); }
        .word { font-size: 2.8rem; font-weight: 300; margin-bottom: 20px; transition: color 0.5s ease; color: #333; display: flex; align-items: baseline; justify-content: center; gap: 0.3em; }
        .word .article { font-size: 0.5em; opacity: 0.6; font-weight: 400; transition: opacity 0.3s ease; }
        .word .main-word { font-weight: 300; display: flex; align-items: center; transition: all 1.5s cubic-bezier(0.25, 0.46, 0.45, 0.94); }
        .word .morpheme { transition: all 1.5s cubic-bezier(0.25, 0.46, 0.45, 0.94); }
        .word .morpheme-separator { opacity: 0; transform: scaleX(0); transition: all 1.5s cubic-bezier(0.25, 0.46, 0.45, 0.94); color: #999; font-weight: 400; margin: 0 0.1em; }
        .word.show-morphemes .morpheme { margin: 0 0.15em; }
        .word.show-morphemes .morpheme-separator { opacity: 1; transform: scaleX(1); }
        .word.hide-articles .article { opacity: 0; }
        .word .article.der { color: #0066cc; } .word .article.die { color: #cc0066; } .word .article.das { color: #666; }
        .word.der .main-word { color: #0066cc; } .word.die .main-word { color: #cc0066; } .word.das .main-word { color: #333; }
        
        .morpheme-translations { margin-top: 15px; opacity: 0; transform: translateY(10px); transition: opacity 0.8s ease-out 0.2s, transform 0.8s ease-out 0.2s; display: flex; justify-content: center; flex-wrap: wrap; gap: 1.5em; min-height: 40px; }
        .morpheme-translations.visible { opacity: 1; transform: translateY(0); }
        .morpheme-translation-item { display: flex; flex-direction: column; align-items: center; line-height: 1.2; }
        .morpheme-translation-item .morpheme-part { font-weight: 500; font-size: 1rem; color: #555; }
        .morpheme-translation-item .translation-part { font-size: 0.85rem; color: #888; font-style: italic; }
        .level-indicator { position: absolute; top: 15px; right: 15px; padding: 4px 8px; border-radius: 3px; font-size: 0.75rem; font-weight: 500; color: white; text-transform: uppercase; letter-spacing: 0.5px; }
        .level-indicator.a1 { background: #666; } .level-indicator.a2 { background: #888; } .level-indicator.b1 { background: #333; } .level-indicator.b2 { background: #000; }
        
        /* Анимации */
        .card.transform-phase .word-container { transform: translateY(40px) scale(1.1); }
        .card.morpheme-phase, .card.sentence-phase, .card.zoom-phase { transform: scale(1.02); }
        .card.morpheme-phase .word-container, .card.sentence-phase .word-container, .card.zoom-phase .word-container { transform: translateY(40px) scale(1.15); }
        .card.final-phase { transform: scale(1.0); }
        .card.final-phase .word-container { transform: translateY(0) scale(1.0); }
        .word-crossfade { transition: opacity 1.5s ease-in-out, transform 1.5s ease-in-out; }
        .word-fade-out { opacity: 0; transform: scale(0.98); }
        @keyframes slideInFromBottom { from { opacity: 0; transform: translateY(20px); } to { opacity: 1; transform: translateY(0); } }
        .card-appear { animation: slideInFromBottom 0.6s ease-out; }
        .translation-appear, .sentence-appear { animation: slideInFromBottom 0.6s ease-out; }

        .translation-container, .sentence-container { min-height: 50px; display: flex; flex-direction: column; justify-content: center; align-items: center; }
        .translation { font-size: 1.6rem; color: #666; margin-bottom: 12px; font-weight: 400; }
        .pronunciation { font-size: 1.1rem; color: #999; font-style: italic; font-weight: 300; }

        /* Стили для предложения */
        .sentence {
            font-size: 1.4rem; color: #4a90e2; margin: 20px 0 12px 0; font-weight: 400;
            font-style: italic; padding: 15px; border-left: 3px solid #4a90e2; background: rgba(74, 144, 226, 0.05);
            width: 100%; text-align: center;
        }
        .sentence-translation { font-size: 1.1rem; color: #888; margin-top: 8px; font-style: normal; }

        .player-controls { background: white; padding: 25px; border-top: 1px solid #e5e5e5; border-bottom: 1px solid #e5e5e5; display: flex; flex-direction: column; align-items: center; gap: 20px; }
        .player-buttons { display: flex; align-items: center; gap: 15px; }
        .player-btn { width: 50px; height: 50px; border: 2px solid #ddd; border-radius: 50%; background: white; cursor: pointer; transition: all 0.2s ease; display: flex; align-items: center; justify-content: center; color: #666; }
        .player-btn svg { transition: transform 0.2s ease; }
        .player-btn:hover { border-color: #999; background: #f8f8f8; }
        .player-btn:active svg { transform: scale(0.9); }
        .player-btn:disabled { border-color: #ddd; background: #f5f5f5; color: #ccc; cursor: not-allowed; }
        .play-pause { width: 60px; height: 60px; border-width: 3px; }
        .play-pause.playing { background: #333; color: white; border-color: #333; }
        .sound-btn.enabled, .translation-sound-btn.enabled, .sentence-sound-btn.enabled { background: #333; color: white; border-color: #333; }
        
        .player-options { display: flex; align-items: center; gap: 20px; flex-wrap: wrap; justify-content: center; }
        .sound-control, .repeat-control, .article-control, .morpheme-control, .morpheme-translation-control, .sentence-control { display: flex; align-items: center; gap: 10px; }
        .control-label { font-size: 0.9rem; color: #666; font-weight: 500; }
        .repeat-selector { padding: 6px 12px; border: 1px solid #ddd; border-radius: 4px; background: white; font-weight: 500; cursor: pointer; transition: all 0.2s ease; color: #666; font-size: 0.85rem; }
        .repeat-selector:hover { border-color: #999; }
        .repeat-selector.active { background: #333; color: white; border-color: #333; }
        .repeat-selector:disabled { opacity: 0.6; cursor: not-allowed; }
        
        .vocabulary-manager { background: white; padding: 25px; border-top: 1px solid #e5e5e5; }
        .vocabulary-buttons { display: flex; gap: 12px; align-items: center; flex-wrap: wrap; justify-content: center; }
        .vocab-btn { padding: 8px 16px; border: 1px solid #ddd; border-radius: 4px; background: white; font-weight: 500; cursor: pointer; transition: all 0.2s ease; font-size: 0.85rem; color: #666; }
        .vocab-btn:hover { border-color: #999; }
        .vocab-btn.load { background: #333; color: white; border-color: #333; }
        
        .hidden-input { display: none; }
        .no-words { text-align: center; color: #999; font-size: 1.1rem; padding: 60px 20px; font-weight: 300; }
        .status-message { background: #f8f8f8; color: #333; padding: 12px 16px; border-radius: 4px; margin: 12px 0; border: 1px solid #e5e5e5; font-size: 0.85rem; }
        .icon { display: inline-block; width: 1em; height: 1em; stroke-width: 0; stroke: currentColor; fill: currentColor; }
        
        @media (max-width: 768px) {
            .header h1 { font-size: 2rem; } .word { font-size: 2.2rem; } .stats { grid-template-columns: 1fr; }
            .level-buttons, .vocabulary-buttons, .block-buttons { justify-content: center; }
            .level-selector { flex-direction: column; align-items: center; gap: 20px; }
            .main-content { padding: 40px 20px; } .card { padding: 40px 20px; }
            .container { margin: 10px; border-radius: 4px; }
            .player-options { flex-direction: column; gap: 15px; } .player-buttons { gap: 20px; }
        }
        @media (max-width: 480px) {
            body { padding: 10px; } .header h1 { font-size: 1.8rem; } .word { font-size: 1.8rem; }
            .block-buttons { gap: 6px; } .block-btn { padding: 6px 10px; font-size: 0.8rem; }
        }
    </style>
</head>
<body>
    <div class="container">
        <div class="header"><h1>🇩🇪 Deutsch Lernen</h1><p>Словарь с морфемами и предложениями</p></div>
        <div class="stats">
            <div class="stat"><div class="stat-number" id="totalWords">0</div><div class="stat-label">Всего слов</div></div>
            <div class="stat"><div class="stat-number" id="studiedToday">0</div><div class="stat-label">Изучено сегодня</div></div>
            <div class="stat"><div class="stat-number" id="accuracy">0%</div><div class="stat-label">Точность</div></div>
        </div>
        <div class="level-selector">
            <div class="level-buttons">
                <span class="block-label"><svg class="icon"><use xlink:href="#icon-levels"></use></svg>Уровни</span>
                <button class="level-btn a1" data-level="A1">A1</button><button class="level-btn a2" data-level="A2">A2</button><button class="level-btn b1 active" data-level="B1">B1</button><button class="level-btn b2" data-level="B2">B2</button>
            </div>
            <div id="levelStatus" class="status-message" style="display: none;"></div>
        </div>
        <div class="block-selector">
            <div class="block-buttons">
                <span class="block-label"><svg class="icon"><use xlink:href="#icon-category"></use></svg>Темы</span>
                <button class="block-btn active" data-theme="all">Все темы</button><button class="block-btn" data-theme="family">Семья</button><button class="block-btn" data-theme="work">Работа</button><button class="block-btn" data-theme="home">Дом</button><button class="block-btn" data-theme="food">Еда</button><button class="block-btn" data-theme="travel">Путешествия</button><button class="block-btn" data-theme="health">Здоровье</button><button class="block-btn" data-theme="education">Учеба</button><button class="block-btn" data-theme="hobbies">Хобби</button><button class="block-btn" data-theme="verbs">Глаголы</button><button class="block-btn" data-theme="emotions">Эмоции</button><button class="block-btn" data-theme="weather">Погода</button>
            </div>
        </div>
        <div class="main-content" id="mainContent"><div id="studyArea"><div class="no-words"><p>Загружаю словарь...</p></div></div></div>
        <div class="player-controls">
            <div class="player-buttons">
                <button class="player-btn" id="prevButton" title="Предыдущее слово"></button>
                <button class="player-btn play-pause" id="toggleButton" title="Остановить/Запустить"></button>
                <button class="player-btn" id="nextButton" title="Следующее слово"></button>
            </div>
            <div class="player-options">
                <div class="sound-control">
                    <button class="player-btn sound-btn enabled" id="soundToggle" title="Звук слова"></button>
                    <button class="player-btn translation-sound-btn enabled" id="translationSoundToggle" title="Звук перевода"></button>
                    <button class="player-btn sentence-sound-btn enabled" id="sentenceSoundToggle" title="Звук предложения"></button>
                </div>
                <div class="repeat-control">
                    <span class="control-label">Повторений:</span>
                    <button class="repeat-selector" data-mode="1">1</button><button class="repeat-selector active" data-mode="2">2</button><button class="repeat-selector" data-mode="3">3</button><button class="repeat-selector" data-mode="random">∞</button>
                </div>
                <div class="article-control"><span class="control-label">Артикли:</span><button class="repeat-selector active" id="toggleArticles" title="Показать/Скрыть артикли">Да/Нет</button></div>
                <div class="morpheme-control"><span class="control-label">Морфемы:</span><button class="repeat-selector active" id="toggleMorphemes" title="Показать/Скрыть разделение">Да/Нет</button></div>
                <div class="morpheme-translation-control"><span class="control-label">Пер. морфем:</span><button class="repeat-selector active" id="toggleMorphemeTranslations" title="Показать/Скрыть перевод морфем">Да/Нет</button></div>
                <div class="sentence-control"><span class="control-label">Предложения:</span><button class="repeat-selector active" id="toggleSentences" title="Показать/Скрыть предложения">Да/Нет</button></div>
            </div>
        </div>
        <div class="vocabulary-manager">
            <div class="vocabulary-buttons">
                <span class="block-label"><svg class="icon"><use xlink:href="#icon-book"></use></svg>Словари</span>
                <button class="vocab-btn load" id="loadFromGithub">Загрузить с GitHub</button><button class="vocab-btn" id="reloadDefaultWords">🔄 Обновить словарь</button><button class="vocab-btn" id="exportWords">Экспорт</button><button class="vocab-btn" id="importWords">Импорт файла</button><input type="file" id="fileInput" class="hidden-input" accept=".json" />
            </div>
        </div>
    </div>

    <!-- SVG Icon Definitions -->
    <svg width="0" height="0" style="position:absolute"><defs>
        <symbol id="icon-play" viewBox="0 0 24 24"><path d="M8 5v14l11-7z"/></symbol>
        <symbol id="icon-pause" viewBox="0 0 24 24"><path d="M6 19h4V5H6v14zm8-14v14h4V5h-4z"/></symbol>
        <symbol id="icon-prev" viewBox="0 0 24 24"><path d="M6 6h2v12H6zm3.5 6 8.5 6V6z"/></symbol>
        <symbol id="icon-next" viewBox="0 0 24 24"><path d="M16 6h2v12h-2zm-3.5 6-8.5 6V6z"/></symbol>
        <symbol id="icon-sound-on" viewBox="0 0 24 24"><path d="M3 9v6h4l5 5V4L7 9H3zm13.5 3c0-1.77-1.02-3.29-2.5-4.03v8.05c1.48-.73 2.5-2.25 2.5-4.02zM14 3.23v2.06c2.89.86 5 3.54 5 6.71s-2.11 5.85-5 6.71v2.06c4.01-.91 7-4.49 7-8.77s-2.99-7.86-7-8.77z"/></symbol>
        <symbol id="icon-sound-off" viewBox="0 0 24 24"><path d="M16.5 12c0-1.77-1.02-3.29-2.5-4.03v2.21l2.45 2.45c.03-.2.05-.41.05-.63zm2.5 0c0 .94-.2 1.82-.54 2.64l1.51 1.51C20.63 14.91 21 13.5 21 12c0-4.28-2.99-7.86-7-8.77v2.06c2.89.86 5 3.54 5 6.71zM4.27 3L3 4.27 7.73 9H3v6h4l5 5v-6.73l4.25 4.25c-.67.52-1.42.93-2.25 1.18v2.06c1.38-.31 2.63-.95 3.69-1.81L19.73 21 21 19.73l-9-9L4.27 3zM12 4L9.91 6.09 12 8.18V4z"/></symbol>
        <symbol id="icon-chat-on" viewBox="0 0 24 24"><path d="M20 2H4c-1.1 0-2 .9-2 2v18l4-4h14c1.1 0 2-.9 2-2V4c0-1.1-.9-2-2-2z"/></symbol>
        <symbol id="icon-chat-off" viewBox="0 0 24 24"><path d="m2.27 1.73 1.28-1.28L22 18.95l-1.27 1.28L18.95 20H4l-4 4V4c0-.28.06-.55.15-.8L2.27 1.73zM20 2H4.05L22 20.03V4c0-1.1-.9-2-2-2z"/></symbol>
        <symbol id="icon-sentence-on" viewBox="0 0 24 24"><path d="M14 2H6c-1.1 0-1.99.9-1.99 2L4 20c0 1.1.89 2 1.99 2H18c1.1 0 2-.9 2-2V8l-6-6zM16 18H8v-2h8v2zm0-4H8v-2h8v2zm-3-5V3.5L18.5 9H13z"/></symbol>
        <symbol id="icon-sentence-off" viewBox="0 0 24 24"><path d="M20.49 20.49L3.51 3.51 2.24 4.78l2.75 2.75H6c-1.1 0-2 .9-2 2v12c0 1.1.9 2 2 2h12c.28 0 .55-.07.8-.18l2.44 2.44 1.27-1.27zM8 18v-2h3.17l2 2H8zm4.83-4H8v-2h1.17l4 4H16v-1.17l-2-2-1.17-1.17zM14 2H6c-1.1 0-2 .9-2 2v.17l2 2V4h7v5h5v5.17l2 2V8l-6-6z"/></symbol>
        <symbol id="icon-levels" viewBox="0 0 24 24"><path d="M3 13h18v-2H3v2zm0 4h18v-2H3v2zm0-8h18V7H3v2z"/></symbol>
        <symbol id="icon-category" viewBox="0 0 24 24"><path d="M4 8h4V4H4v4zm6 12h4v-4h-4v4zm-6 0h4v-4H4v4zm0-6h4v-4H4v4zm6 0h4v-4h-4v4zm6-10v4h4V4h-4zm-6 4h4V4h-4v4zm6 6h4v-4h-4v4zm0 6h4v-4h-4v4z"/></symbol>
        <symbol id="icon-book" viewBox="0 0 24 24"><path d="M18 2H6c-1.1 0-2 .9-2 2v16c0 1.1.9 2 2 2h12c1.1 0 2-.9 2-2V4c0-1.1-.9-2-2-2zM9 4h2v5l-1-0.75L9 9V4zm6 12H9v-2h6v2zm3-4H9v-2h9v2zm0-4H9V8h9v2z"/></symbol>
    </defs></svg>

    <script>
        class VocabularyApp {
            constructor() {
                this.allWords = [];
                this.currentWord = null;
                this.studiedToday = parseInt(localStorage.getItem('studiedToday')) || 0;
                this.accuracy = JSON.parse(localStorage.getItem('accuracy')) || { correct: 0, total: 0 };
                this.lastStudyDate = localStorage.getItem('lastStudyDate');
                this.isAutoPlaying = true;
                this.autoPlayTimeout = null;
                
                // --- State properties ---
                this.soundEnabled = JSON.parse(localStorage.getItem('soundEnabled')) !== false;
                this.translationSoundEnabled = JSON.parse(localStorage.getItem('translationSoundEnabled')) !== false;
                this.sentenceSoundEnabled = JSON.parse(localStorage.getItem('sentenceSoundEnabled')) !== false;
                this.repeatMode = localStorage.getItem('repeatMode') || '2';
                this.selectedLevels = JSON.parse(localStorage.getItem('selectedLevels')) || ['B1'];
                this.selectedTheme = localStorage.getItem('selectedTheme') || 'all';
                this.showArticles = JSON.parse(localStorage.getItem('showArticles')) !== false;
                this.showMorphemes = JSON.parse(localStorage.getItem('showMorphemes')) !== false;
                this.showMorphemeTranslations = JSON.parse(localStorage.getItem('showMorphemeTranslations')) !== false;
                this.showSentences = JSON.parse(localStorage.getItem('showSentences')) !== false;

                this.speechInitialized = false;
                this.wordHistory = [];
                this.currentHistoryIndex = -1;
                
                const today = new Date().toDateString();
                if (this.lastStudyDate !== today) { this.studiedToday = 0; localStorage.setItem('lastStudyDate', today); }
                
                this.init();
            }

            init() {
                this.setupIcons(); 
                this.loadVocabulary(); 
                this.bindEvents(); 
                this.updateUI(); 
                this.startAutoPlay();
            }
            
            setupIcons() {
                document.getElementById('prevButton').innerHTML = `<svg class="icon" width="18px" height="18px"><use xlink:href="#icon-prev"></use></svg>`;
                document.getElementById('nextButton').innerHTML = `<svg class="icon" width="18px" height="18px"><use xlink:href="#icon-next"></use></svg>`;
                this.updateToggleButton(); 
                this.updateSoundButton(); 
                this.updateTranslationSoundButton();
                this.updateSentenceSoundButton();
            }
            
            updateUI() {
                this.updateLevelButtons(); 
                this.updateThemeButtons(); 
                this.updateSoundButton(); 
                this.updateTranslationSoundButton(); 
                this.updateSentenceSoundButton();
                this.updateRepeatButtons(); 
                this.updateArticleButton(); 
                this.updateMorphemeButton(); 
                this.updateMorphemeTranslationsButton(); 
                this.updateSentenceButton();
                this.updateNavigationButtons();
            }

            loadVocabulary() {
                const savedWords = localStorage.getItem('germanWords');
                try {
                    if (savedWords) {
                        const parsed = JSON.parse(savedWords);
                        if (parsed.length > 200 && parsed.every(w => 'sentence' in w)) {
                             this.allWords = parsed;
                             this.updateStats(); 
                             this.showMessage(`Загружен сохраненный словарь: ${this.allWords.length} слов`);
                             return;
                        }
                    }
                } catch (e) { console.error('Ошибка загрузки сохраненного словаря:', e); }
                this.loadDefaultVocabulary();
            }

            loadDefaultVocabulary() {
                this.allWords = [
